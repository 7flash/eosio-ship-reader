{
  "name": "@blockmatic/eosio-ship-reader",
  "version": "0.0.11",
  "description": "Multi-threaded reactive eosio state reader for NodeJS apps",
  "repository": "git@github.com:blockmatic/eosio-ship-reader.git",
  "author": "blockmatic.io",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "tsc && ts-node-dev --no-deps --respawn examples/index",
    "build": "rimraf dist && tsc",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(js|ts|tsx)\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "dist": "node dist/examples",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run format",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@blockmatic/eslint-config": "^1.3.11",
    "@blockmatic/prettier-config": "^1.3.3",
    "@blockmatic/tsconfig": "^1.8.4",
    "@types/node": "^14.14.8",
    "@types/text-encoding": "^0.0.35",
    "@types/ws": "^7.4.0",
    "eslint": "6",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "ts-node-dev": "^1.0.0",
    "typescript": "^3.7",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@eosrio/node-abieos": "^2.1.0-beta.0",
    "@types/node-fetch": "^2.5.7",
    "eosjs": "^21.0.3",
    "node-fetch": "^2.6.1",
    "node-worker-threads-pool": "^1.4.3",
    "p-queue": "^6.6.2",
    "rxjs": "^6.6.3",
    "text-encoding": "^0.7.0",
    "winston": "^3.3.3",
    "ws": "^7.4.0"
  }
}
